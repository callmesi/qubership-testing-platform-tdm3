<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="RELEASE_LOCK" author="tdm3 support/dev team">
        <update tableName="DATABASECHANGELOGLOCK">
            <column name="LOCKED" valueBoolean="false"/>
            <column name="LOCKGRANTED" valueComputed="NULL"/>
            <column name="LOCKEDBY" valueComputed="NULL"/>
            <where>ID = 1</where>
        </update>
    </changeSet>

</databaseChangeLog>
